// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+0",
        "command": "workbench.action.nextEditorInGroup"
    },
    {
        "key": "ctrl+9",
        "command": "workbench.action.previousEditorInGroup"
    },
    {
        "key": "ctrl+0",
        "command": "-workbench.action.lastEditorInGroup"
    },
    {
        "key": "ctrl+a j",
        "command": "workbench.action.splitEditorDown",
        "when": "!terminalFocus"
    },
    {
        "key": "ctrl+a h",
        "command": "workbench.action.splitEditorLeft",
        "when": "!terminalFocus"
    },
    {
        "key": "ctrl+a l",
        "command": "workbench.action.splitEditorRight",
        "when": "!terminalFocus"
    },
    {
        "key": "ctrl+a k",
        "command": "workbench.action.splitEditorUp",
        "when": "!terminalFocus"
    },
    {
        "key": "ctrl+a x",
        "command": "workbench.action.closeEditorsInGroup",
        "when": "!terminalFocus"
    },
    {
        "key": "ctrl+k",
        "command": "-deleteAllRight",
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "key": "ctrl+tab",
        "command": "workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "ctrl+`",
        "command": "-workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.focusAboveGroup"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.focusBelowGroup"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.action.focusLeftGroup"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.focusRightGroup"
    },
    {
        "key": "ctrl+shift+9",
        "command": "workbench.action.moveActiveEditorGroupLeft"
    },
    {
        "key": "ctrl+shift+0",
        "command": "workbench.action.moveActiveEditorGroupRight"
    },
    {
        "key": "ctrl+shift+-",
        "command": "workbench.action.joinAllGroups"
    },
    {
        "key": "ctrl+=",
        "command": "workbench.action.increaseViewSize"
    },
    {
        "key": "ctrl+-",
        "command": "workbench.action.decreaseViewSize"
    },
    {
        "key": "ctrl+\\",
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "ctrl+a enter",
        "command": "workbench.action.toggleEditorWidths",
        "when": "!terminalFocus"
    },
    {
        "key": "shift+h",
        "command": "list.collapseAll",
        "when": "listFocus && !inputFocus"
    },
    {
        "key": "ctrl+/",
        "command": "search.action.focusSearchList",
        "when": "searchViewletVisible"
    },
    {
        "key": "ctrl+/",
        "command": "workbench.view.search.focus",
        "when": "listFocus || !searchViewletVisible"
    },
    {
        "key": "ctrl+o",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "ctrl+backspace",
        "command": "workbench.action.revertAndCloseActiveEditor"
    },
    {
        "key": "ctrl+enter",
        "command": "workbench.action.toggleZenMode"
    },
    {
        "key": "ctrl+g",
        "command": "workbench.action.focusSideBar"
    },
    {
        "key": "ctrl+n",
        "command": "workbench.action.quickOpenNavigateNextInFilePicker",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "ctrl+p",
        "command": "-workbench.action.quickOpenNavigateNextInFilePicker",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "ctrl+p",
        "command": "workbench.action.quickOpenNavigatePreviousInFilePicker",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "alt+n",
        "command": "workbench.action.files.newUntitledFile"
    },
    {
        "key": "ctrl+n",
        "command": "-workbench.action.files.newUntitledFile"
    },
    {
        "key": "ctrl+shift+l",
        "command": "workbench.action.moveEditorToNextGroup"
    },
    {
        "key": "ctrl+alt+right",
        "command": "-workbench.action.moveEditorToNextGroup"
    },
    {
        "key": "ctrl+shift+h",
        "command": "workbench.action.moveEditorToPreviousGroup"
    },
    {
        "key": "ctrl+alt+left",
        "command": "-workbench.action.moveEditorToPreviousGroup"
    },
    {
        "key": "ctrl+p",
        "command": "workbench.action.quickOpen"
    },
    {
        "key": "cmd+p",
        "command": "-workbench.action.quickOpen"
    },
    {
        "key": "ctrl+shift+p",
        "command": "workbench.action.showCommands"
    },
    {
        "key": "shift+cmd+p",
        "command": "-workbench.action.showCommands"
    },
    {
        "key": "ctrl+` shift+k",
        "command": "workbench.action.openGlobalKeybindingsFile"
    },
    {
        "key": "ctrl+` k",
        "command": "workbench.action.openGlobalKeybindings"
    },
    {
        "key": "ctrl+k ctrl+s",
        "command": "-workbench.action.openGlobalKeybindings"
    },
    {
        "key": "ctrl+` shift+s",
        "command": "workbench.action.openSettingsJson"
    },
    {
        "key": "ctrl+` s",
        "command": "workbench.action.openSettings"
    },
    {
        "key": "ctrl+,",
        "command": "-workbench.action.openSettings"
    },
    // Terminal Commands
    {
        "key": "ctrl+5",
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "${workspaceFolder}/tools/correct_deps ${relativeFile}\u000D"
        }
    },
    // Bazel target starters
    {
        "key": "ctrl+b",
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "./bazel build //${relativeFileDirname}"
        },
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+t",
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "./bazel test //${relativeFileDirname}"
        },
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+r",
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "./bazel run //${relativeFileDirname}"
        },
        "when": "terminalFocus"
    },
    // Git files open
    {
        "key": "ctrl+shift+o",
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "code $(git diff @{upstream} HEAD --name-only)\u000D"
        }
    },
    {
        "key": "ctrl+shift+m",
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "code $(git diff --name-only)\u000D"
        },
    },
]
